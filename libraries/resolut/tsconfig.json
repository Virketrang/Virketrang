{
    "extends": "../../tsconfig.base.json",
    "compilerOptions": {
        "target": "ES6",
        "lib": ["ES5", "ES2015", "ES2016", "DOM", "ESNext"],
        "jsx": "react-jsx",
        "module": "ES2015",
        "moduleResolution": "node",
        "types": ["node"],
        "declaration": true,
        "sourceMap": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "preserveSymlinks": true,
        "resolveJsonModule": true,
        "allowSyntheticDefaultImports": true
    }
}
