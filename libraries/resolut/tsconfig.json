{
    "extends": "../../tsconfig.base.json",
    "compilerOptions": {
        "target": "ESNext",
        "lib": ["DOM", "DOM.Iterable", "ESNext"],
        "module": "ESNext",
        "moduleResolution": "node",
        "emitDeclarationOnly": true,
        "outDir": "dist",
        "declaration": true,
        "sourceMap": true,
        "skipLibCheck": true,
        "preserveSymlinks": true,
        "resolveJsonModule": true,
        "allowUmdGlobalAccess": true,
        "paths": {}
    }
}
