{
  "name": "resolut",
  "version": "0.0.0",
  "description": "A component library for ReactJS and NextJS",
  "homepage": "https://www.virketrang.com",
  "bugs": {
    "url": "https://github.com/virketrang/virketrang/issues",
    "email": "sigfred@virketrang.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/virketrang/virketrang",
    "directory": "libraries/resolut"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/",
    "access": "public"
  },
  "main": "dist/resolut.cjs.js",
  "module": "dist/resolut.es.js",
  "types": "dist/resolut.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/resolut.es.js",
      "require": "./dist/resolut.cjs.js"
    },
    "./style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "scripts": {
    "dev": "storybook dev --port 8000 --watch",
    "start": "pnpm run dev",
    "build": "pnpm run build:client && pnpm run build:server && pnpm run build:colors",
    "build:client": "nx build @resolut/client",
    "build:server": "nx build @resolut/server",
    "build:colors": "nx build @resolut/colors",
    "build:storybook": "storybook build",
    "watch": "pnpm run watch:client && pnpm run watch:server && pnpm run watch:colors",
    "watch:client": "nx watch @resolut/client",
    "watch:server": "nx watch @resolut/server",
    "watch:colors": "nx watch @resolut/colors"
  },
  "keywords": [
    "NextJS",
    "React",
    "Component Library"
  ],
  "author": {
    "name": "Sigfred Frey NÃ¸rgaard Knudsen",
    "email": "sigfred@virketrang.com",
    "url": "https://www.virketrang.com/"
  },
  "license": "MIT",
  "devDependencies": {
    "@parcel/packager-ts": "^2.8.3",
    "@parcel/transformer-sass": "^2.8.3",
    "@parcel/transformer-typescript-types": "^2.8.3",
    "@storybook/addon-actions": "7.0.7",
    "@storybook/addon-essentials": "7.0.7",
    "@storybook/addon-interactions": "7.0.7",
    "@storybook/addon-links": "7.0.7",
    "@storybook/cli": "7.0.7",
    "@storybook/react": "7.0.7",
    "@storybook/react-vite": "7.0.7",
    "@storybook/testing-library": "0.1.0",
    "@storybook/types": "7.0.7",
    "@types/node": "^18.15.5",
    "@types/react": "^18.0.28",
    "parcel": "^2.8.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.58.3",
    "storybook": "7.0.7",
    "typescript": "^5.0.4",
    "typescript-plugin-css-modules": "^5.0.1",
    "vitest": "^0.30.1",
    "webpack": "^5.76.3"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  }
}
