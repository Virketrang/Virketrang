{
    "extends": "../tsconfig.base.json",
    "compilerOptions": {
        "module": "CommonJS",
        "declaration": true,
        "allowSyntheticDefaultImports": true,
        "target": "ESNext",
        "sourceMap": true,
        "outDir": "./dist",
        "diagnostics": false,
        "extendedDiagnostics": false,
        "typeRoots": ["./server/unimport.d.ts"],
        "incremental": true,
        "skipLibCheck": true,
        "strictBindCallApply": false
    }
}
