{
    "name": "@virketrang/virketrang",
    "version": "0.0.0",
    "description": "The Virketrang monorepo",
    "license": "MIT",
    "private": true,
    "packageManager": "pnpm@8.6.0",
    "engines": {
        "node": ">=19.8.1",
        "yarn": "please-use-pnpm",
        "npm": "please-use-pnpm",
        "pnpm": ">=8.6.0"
    },
    "author": {
        "name": "Sigfred Frey Nørgaard Knudsen",
        "email": "sigfred@virketrang.com",
        "url": "https://www.virketrang.com/staff/sigfred"
    },
    "contributors": [
        {
            "name": "Isabella Liedecke Mørck",
            "email": "isabella@virketrang.com",
            "url": "https://www.virketrang.com/staff/isabella"
        }
    ],
    "keywords": [
        "virketrang",
        "monorepo",
        "react",
        "typescript",
        "nextjs",
        "nestjs",
        "postgresql",
        "typeorm",
        "sass",
        "nx",
        "docker",
        "storybook",
        "pnpm",
        "workspace"
    ],
    "homepage": "https://www.virketrang.com",
    "bugs": {
        "url": "https://github.com/virketrang/virketrang/issues",
        "email": "sigfred@virketrang.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/virketrang/virketrang"
    },
    "scripts": {
        "GENERAL": "-------------- GENERAL --------------",
        "preinstall": "npx only-allow pnpm",
        "reinstall": "rm -rf node_modules/",
        "pnx": "nx",
        "APPS": "-------------- APPS --------------",
        "server": "nest start server",
        "website": "nx dev @virketrang/website",
        "dashboard": "nx dev @virketrang/dashboard",
        "strive": "pnpm run strive:server && pnpm run strive:client",
        "strive:client": "nx dev @virketrang/strive-client",
        "strive:server": "nx dev @virketrang/strive-server",
        "liedecke-noergaard": "nx dev @virketrang/liedecke-noergaard",
        "LIBRARIES": "---------- LIBRARIES ----------",
        "resolut:build": "nx build resolut",
        "resolut:client:build": "nx build @resolut/client",
        "resolut:client": "nx dev @resolut/client",
        "resolut:server": "nx dev @resolut/server",
        "UPDATE": "---------- UPDATE ----------",
        "update:all": "pnpm run update:storybook && pnpm run update:next && pnpm run update:parcel && pnpm run update:nx",
        "update:storybook": "npx storybook@latest upgrade",
        "update:next": "pnpm --recursive update next react react-dom eslint-config-next --latest",
        "update:parcel": "pnpm --recursive update \"@parcel/*\" parcel",
        "update:nx": "pnpm update \"@nx/*\" \"@nrwl/*\""
    },
    "devDependencies": {
        "@nrwl/cli": "^15.9.2",
        "@nrwl/nx-cloud": "latest",
        "@nx/js": "16.3.2",
        "@nx/react": "^16.3.2",
        "@nx/vite": "^16.3.2",
        "@nx/workspace": "^16.1.4",
        "@parcel/transformer-sass": "2.9.1",
        "@parcel/transformer-typescript-types": "2.9.1",
        "@testing-library/react": "14.0.0",
        "@types/node": "18.14.2",
        "@types/react": "18.0.28",
        "@types/react-dom": "18.0.11",
        "@vitejs/plugin-react": "^3.0.0",
        "@vitest/coverage-c8": "^0.31.0",
        "@vitest/ui": "^0.31.0",
        "eslint": "^8.40.0",
        "eslint-plugin-import": "2.27.5",
        "eslint-plugin-jsx-a11y": "6.7.1",
        "eslint-plugin-react": "7.32.2",
        "eslint-plugin-react-hooks": "4.6.0",
        "husky": "^8.0.3",
        "jsdom": "~20.0.3",
        "nx": "16.1.4",
        "pnpm": "^8.6.0",
        "prettier": "^2.8.4",
        "rimraf": "^3.0.2",
        "sass": "^1.55.0",
        "typescript": "^5.0.4",
        "vite": "^4.3.4",
        "vite-plugin-dts": "~1.7.1",
        "vite-plugin-eslint": "^1.8.1",
        "vite-tsconfig-paths": "^4.0.2",
        "vitest": "^0.31.0"
    },
    "dependencies": {
        "react": "18.2.0",
        "react-dom": "18.2.0"
    }
}
