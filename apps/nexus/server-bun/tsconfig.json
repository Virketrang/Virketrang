{
    "extends": "../../../tsconfig.base.json",
    "compilerOptions": {
        "lib": ["ESNext"],
        "target": "esnext",
        "moduleResolution": "bundler",
        "moduleDetection": "force",
        "noEmit": true,
        "composite": true,
        "downlevelIteration": true,
        "skipLibCheck": true,
        "allowJs": true,
        "allowImportingTsExtensions": true,
        "types": ["bun-types"]
    }
}
